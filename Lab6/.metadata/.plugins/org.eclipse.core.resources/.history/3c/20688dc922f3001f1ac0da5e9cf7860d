package ch8ex10;

public class DrugTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 3;
		int employee[] = new int[num];
		int test;
		
		for (int i = 0; i < num; i++) {
			test = 1 + (int)(Math.random() * num);
			employee[test-1]++; 
			
		}
		for (int e : employee) {
			System.out.println(e);
		}
		
	}

}
