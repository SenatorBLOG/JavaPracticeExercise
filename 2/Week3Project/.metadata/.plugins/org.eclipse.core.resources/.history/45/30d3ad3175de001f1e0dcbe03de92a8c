package practice;

public class Employee {
	 private int empID;
	 private NameAndAddress nAdd;
	 
	public Employee(int empID, NameAndAddress nAdd) {
		this.empID = empID;
		this.nAdd = nAdd;
	}
	public Employee(int empID, String n, String ad, String pc) {
		this.empID = empID;
		nAdd = new NameAndAddress(n,ad,pc);
	}
	 
}
